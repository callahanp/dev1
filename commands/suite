#!/bin/bash
# source only

set +x
source $DEV1_COMMANDS/set_current_context $1 $2

if [[ ! "$2" == "" ]]; then

  source $DEV1/commands/task $current_suite $2
  cd $DEV1_SUITES/$current_suite/tasks/$current_task
  echo $(pwd) > $DEV1_USER_DATA/${current_suite}_task
  cd $(cat $DEV1_USER_DATA/${current_suite}_task)
fi
