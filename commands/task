#!/bin/bash
if [[ ! "$DEV1_DEBUG" == "" ]]; then
  echo "starting $DEV1/commands/task $@"
  set -x
fi
#set -x #uncomment for debugging
current_suite=$(cat $DEV1_USER_DATA/current_suite)
if [[ "$1" == "$current_suite" ]]; then
  task="$2"
else
  task="$1"
fi
source $DEV1_COMMANDS/set_current_context $current_suite $task
if [[ ! "$DEV1_DEBUG" == "" ]]; then
  echo "sending   $DEV1/commands/run $@"
  set +x
fi